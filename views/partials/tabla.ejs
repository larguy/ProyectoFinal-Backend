<% if(productos.length !== 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>ID</th> <th>Nombre</th> <th>Descripcion</th> <th>Codigo</th> <th>URL</th> <th>Precio</th> <th>Stock</th> <th>Alta</th> </tr>
                <% for (const producto in productos ) { %>
            <tr>
            <tr>
                <td> <%= productos[producto].id %> </td>
                <td> <%= productos[producto].nombre %> </td>
                <td> <%= productos[producto].descripcion %> </td>
                <td> <%= productos[producto].codigo %> </td>
                <td> <%= productos[producto].imgUrl %> </td>
                <td> <%= productos[producto].precio %></td>
                <td> <%= productos[producto].stock %></td>
                <td> <%= productos[producto].timestamp %></td>
            </tr>
            <% } %>
        </table>
    </div>
<% } else{ %> 
   <h3 style="color: black">no existen datos</h3>
<% }%>

